{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "no-namespace": false,
    "interface-over-type-literal": false,
    "object-literal-sort-keys": false,
    "ordered-imports": [true, { "import-sources-order": "lowercase-last" }]
  },
  "rulesDirectory": []
}
